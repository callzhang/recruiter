{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug wasm export",
      "type": "debugpy",
      "request": "launch",
      "program": "scripts/debug_wasm_export.py",
      "console": "integratedTerminal",
      "args": [
        "--use-local-wasm"
      ]
    },
    {
      "name": "Boss service (start_service.py)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/start_service.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "subProcess": true,
      "env": {
        "HEADLESS": "0",
        "PYTHONUNBUFFERED": "1"
      }
    }
  ]
}
